{
  "name": "pluginever/wc-serial-number",
  "description": "The best WordPress Plugin to sell license keys, redeem cards and other secret numbers!",
  "keywords": [
    "wordpress",
    "plugin"
  ],
  "homepage": "https://www.pluginever.com",
  "type": "wordpress-plugin",
  "license": "GPL-3.0",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "pluginever",
      "email": "support@pluginever.com"
    }
  ],
  "repositories": [
    {
      "url": "https://github.com/byteever/phpcs-composer.git",
      "type": "git"
    },
    {
      "url": "https://github.com/byteever/plugin-framework",
      "type": "git"
    }
  ],
  "require": {
    "php": ">=7.0",
    "byteever/plugin-framework": "dev-master",
    "composer/installers": "~1.7"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "byteever/phpcs-composer": "dev-master",
    "phpunit/phpunit": "^6.5"
  },
  "autoload": {
    "psr-4": {
      "PluginEver\\WooCommerce\\WCSerialNUmbers\\": "src"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    },
    "sort-packages": true
  },
  "scripts": {
    "pre-commit": [
      "@phpcs"
    ],
    "phpcs": "phpcs -s -p .",
    "phpcbf": "phpcbf -p ."
  },
  "extra": {
    "scripts-description": {
      "phpcs": "Analyze code against the WordPress coding standards with PHP_CodeSniffer",
      "phpcbf": "Fix coding standards warnings/errors automatically with PHP Code Beautifier"
    }
  }
}
